SPEC javax.crypto.CipherOutputStream
OBJECTS
    byte[] byte;
    int off;
    int len;
    int b;
    java.io.OutputStream os;
    javax.crypto.Cipher ciph;

EVENTS
    c1: CipherOutputStream(os, ciph);
    
    w1: write(b);
    w2: write(byte);
    w3: write(byte, off, len);
    Writes := w1 | w2 | w3;

    c: close();

ORDER
    c1, Writes+, c

CONSTRAINTS
    len > off;

REQUIRES
    initializedCipher[ciph];

ENSURES
    cipheredOutputStream[os, ciph];
