SPEC javax.crypto.CipherOutputStream
OBJECTS
    byte[] byte;
    int off;
    int len;
    int b;
    java.io.OutputStream os;
    javax.crypto.Cipher c;

EVENTS
    c1: CipherOutputStream(os, c)

    w1: write(b);
    w2: read(byte);
    w3: read(byte, off, len);
    Writes := w1 | w2 | w3;

    c: close();

ORDER
    c1, Writes, c

CONSTRAINTS
    len > off;

REQUIRES
    initializedCipher[c];

ENSURES
    cipheredOutputStream[os, c];
