SPEC java.security.DigestInputStream
OBJECTS
    boolean on;
    byte[] b;
    int off;
    int len;
    java.io.InputStream is;
    java.security.MessageDigest md;

EVENTS
    c1: DigestInputStream(is, md);

    r1: read();
    r2: read(b, off, len);
    Reads := r1 | r2;

    c: close();

FORBIDDEN
    on(on);

ORDER
    c1, Reads, c

CONSTRAINTS
    len > off;

ENSURES
    digestedInputStream[is, md];
