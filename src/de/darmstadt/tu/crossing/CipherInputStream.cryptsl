SPEC javax.crypto.CipherInputStream
OBJECTS
    byte[] b;
    int off;
    int len;
    java.io.InputStream is;
    javax.crypto.Cipher c;

EVENTS
    c1: CipherInputStream(is, c);

    r1: read();
    r2: read(b);
    r3: read(b, off, len);
    Reads := r1 | r2 | r3;

    c: close();

ORDER
    c1, Reads+, c

CONSTRAINTS
    len > off;

REQUIRES
    initializedCipher[c];

ENSURES
    cipheredInputStream[is, c];
