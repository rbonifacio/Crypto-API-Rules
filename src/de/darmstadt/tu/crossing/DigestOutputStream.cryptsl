SPEC java.security.DigestOutputStream
OBJECTS
    byte[] byte;
    int off;
    int len;
    int b;
    java.io.OutputStream os;
    java.security.MessageDigest md;

EVENTS
    c1: DigestOutputStream(os, md);

    w1: write(b);
    w2: write(byte, off, len);
    Writes := w1 | w2;

    c: close();

FORBIDDEN
    on(boolean);

ORDER
    c1, Writes, c

CONSTRAINTS
    len > off;

ENSURES
    digestedOutputStream[os, md];
